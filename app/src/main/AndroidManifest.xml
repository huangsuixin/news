    <!-- 网络访问权限 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="com.app.huangsuixin1506101066.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_blue"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_blue"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.app.huangsuixin1506101066.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.app.huangsuixin1506101066.activity.LoginActivity" />
        <activity android:name="com.app.huangsuixin1506101066.activity.NewsContentView"></activity>
    </application>

</manifest>